<div class="conteiner">
  <h3 class="col-md-3 offset-3">
    会員一覧
  </h3>
  <div class="col-md-9 offset-4">
    <table>
      <thead>
        <tr>
          <th></th>
          <th></th>
          <th>氏名</th>
          <th></th>
          <th>メールアドレス</th>
        </tr>
      </thead>
      <tbody>
        <% @customers.each do |customer| %>
        <tr>
          <td><%= customer.id %></td>
          <td></td>
          <td>
            <%= link_to admin_customer_path(customer.id) do %>
            <%= customer.last_name %><%= customer.first_name %>
            <% end %>
          </td>
          <td></td>
          <td><%= customer.email %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
    </div>
  </div>
</div>